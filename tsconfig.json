{
    "compilerOptions": {
      "jsx": "react-jsx",  
      "module": "ESNext",
      "target": "ESNext",
      "moduleResolution": "Node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      },
      "allowJs": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  